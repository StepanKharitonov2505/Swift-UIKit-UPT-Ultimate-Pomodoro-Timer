# platform :ios, '9.0'

def rxSwift
  pod 'RxSwift'
end

def rxCocoa
  pod 'RxCocoa'
end

def application
  pod 'SnapKit'
end

def animation
  pod 'lottie-ios'
end

target 'UPT Ultimate Pomodoro Timer' do
  use_frameworks!
  rxSwift
  rxCocoa
  application
  animation
end
